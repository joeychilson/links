{
    "version": "1",
    "packages": [
        {
            "path": "database",
            "schema": "database/migrations",
            "queries": "database/queries",
            "engine": "postgresql",
            "sql_package": "pgx/v5"
        }
    ],
    "overrides": [
        {
            "db_type": "uuid",
            "go_type": "github.com/google/uuid.UUID"
        }
    ]
}